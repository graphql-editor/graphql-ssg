{
  "compilerOptions": {
    "incremental": true,
    "target": "esnext",
    "module": "ES2020",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "noEmit": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "https://cdn.skypack.dev/remarkable": [
        "pages/typings/remarkable/index.d.ts"
      ],
      "https://cdn.skypack.dev/react": [
        "pages/typings/react/index.d.ts"
      ],
      "https://cdn.skypack.dev/react-dom": [
        "pages/typings/react-dom/index.d.ts"
      ]
    }
  },
  "include": [
    "./pages/**/*.tsx",
    "./pages/**/*.jsx",
    "./pages/**/*.ts",
    "./pages/**/*.js",
    "./pages/graphql-ssg.d.ts"
  ]
}